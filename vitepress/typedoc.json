{
	"out": "src/API",
	"entryPoints": ["../packages/*"],
	"exclude": [
		"../packages/core",
		"../packages/codegen-tests",
		"../packages/scripts",
		"../packages/utils"
	],
	"name": "Yantrix API",
	"entryPointStrategy": "packages",
	"includeVersion": false,
	"plugin": ["typedoc-plugin-markdown"],
	"readme": "none",
	"modulesFileName": "index",
	"useCodeBlocks": true,
	"disableSources": true,
	"textContentMappings": {
		"title.indexPage": "API"
	},
	"entryFileName": "index",
	"outputFileStrategy": "members",
	"excludeScopesInPaths": true
}
